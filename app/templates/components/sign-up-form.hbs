<form id="sign-up" class="col-md-offset-4 col-md-4 form-horizontal" {{action "submit" on='submit'}}>
  <legend>Setup Your Account</legend>
    <div class="">
    {{email-input email=credentials.email}}
    {{password-input password=credentials.password}}
    {{password-confirmation-input password=credentials.passwordConfirmation}}
    <div class="container row">
      <div class="col-sm-3">
        {{#link-to "sign-up-p2"}}
          <button type="button" class="btn btn-lg btn-block btn-warning">Previous</button>
        {{/link-to}}
      </div>
      <div class="col-sm-3">
        <button type="submit" class="btn btn-lg btn-block btn-primary">Sign Up</button>
      </div>
    </div>


  </div>
</form>
