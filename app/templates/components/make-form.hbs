<form id="make-form" class="col-md-offset-4 col-md-4 form-horizontal" {{action "submit" on='submit'}}>
  <div class="">
    {{make-select selectedOption=makeform.nhtsa_make_id model=model}}
    {{car-year-input year=makeform.year}}
    <div class="container row">
      <div class="col-sm-3">
        <button type="button" class="btn btn-lg btn-block btn-warning">Previous</button>
      </div>
      <div class="col-sm-3">
        <button type="submit" class="btn btn-lg btn-block btn-primary">Next</button>
      </div>
    </div>
  </div>
</form>
